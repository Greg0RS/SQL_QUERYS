CREATE TABLE amz_vat_import (
	id SERIAL PRIMARY KEY,
	UNIQUE_ACCOUNT_IDENTIFIER VARCHAR(20) NULL,
	ACTIVITY_PERIOD DATE NULL,
	SALES_CHANNEL VARCHAR(10),
	MARKETPLACE VARCHAR(20),
	PROGRAM_TYPE VARCHAR(20),
	TRANSACTION_TYPE VARCHAR(20),
	TRANSACTION_EVENT_ID VARCHAR(100),
	ACTIVITY_TRANSACTION_ID VARCHAR(100),
	TAX_CALCULATION_DATE DATE NULL,
	TRANSACTION_DEPART_DATE DATE NULL,
	TRANSACTION_ARRIVAL_DATE DATE NULL,
	TRANSACTION_COMPLETE_DATE DATE NULL,
	SELLER_SKU VARCHAR(100),
	ASIN VARCHAR(20),
	ITEM_DESCRIPTION TEXT,
	ITEM_MANUFACTURE_COUNTRY VARCHAR(20) NULL,
	QTY SMALLINT,
	ITEM_WEIGHT INT NULL,
	TOTAL_ACTIVITY_WEIGHT INT NULL,
	COST_PRICE_OF_ITEMS DECIMAL (8,2) NULL,
	PRICE_OF_ITEMS_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	PROMO_PRICE_OF_ITEMS_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	TOTAL_PRICE_OF_ITEMS_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	SHIP_CHARGE_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	PROMO_SHIP_CHARGE_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	TOTAL_SHIP_CHARGE_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	GIFT_WRAP_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	PROMO_GIFT_WRAP_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	TOTAL_GIFT_WRAP_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	TOTAL_ACTIVITY_VALUE_AMT_VAT_EXCL DECIMAL (8,2) NULL,
	PRICE_OF_ITEMS_VAT_RATE_PERCENT SMALLINT NULL,
	PRICE_OF_ITEMS_VAT_AMT DECIMAL (8,2) NULL,
	PROMO_PRICE_OF_ITEMS_VAT_AMT DECIMAL (8,2) NULL,
	TOTAL_PRICE_OF_ITEMS_VAT_AMT DECIMAL (8,2) NULL,
	SHIP_CHARGE_VAT_RATE_PERCENT SMALLINT NULL,
	SHIP_CHARGE_VAT_AMT DECIMAL (8,2) NULL,
	PROMO_SHIP_CHARGE_VAT_AMT DECIMAL (8,2) NULL,
	TOTAL_SHIP_CHARGE_VAT_AMT DECIMAL (8,2) NULL,
	GIFT_WRAP_RATE_PERCENT SMALLINT NULL,
	GIFT_WRAP_VAT_AMT DECIMAL (8,2) NULL,
	PROMO_GIFT_WRAP_VAT_AMT DECIMAL (8,2) NULL,
	TOTAL_GIFT_WRAP_VAT_AMT DECIMAL (8,2) NULL,
	TOTAL_ACTIVITY_VALUE_VAT_AMT DECIMAL (8,2) NULL,
	PRICE_OF_ITEMS_AMT_VAT_INCL DECIMAL (8,2) NULL,
	PROMO_PRICE_OF_ITEMS_AMT_VAT_INCL DECIMAL (8,2) NULL,
	TOTAL_PRICE_OF_ITEMS_AMT_VAT_INCL DECIMAL (8,2) NULL,
	SHIP_CHARGE_AMT_VAT_INCL DECIMAL (8,2) NULL,
	PROMO_SHIP_CHARGE_AMT_VAT_INCL DECIMAL (8,2) NULL,
	TOTAL_SHIP_CHARGE_AMT_VAT_INCL DECIMAL (8,2) NULL,
	GIFT_WRAP_AMT_VAT_INCL DECIMAL (8,2) NULL,
	PROMO_GIFT_WRAP_AMT_VAT_INCL DECIMAL (8,2) NULL,
	TOTAL_GIFT_WRAP_AMT_VAT_INCL DECIMAL (8,2) NULL,
	TOTAL_ACTIVITY_VALUE_AMT_CAT_INCL DECIMAL (8,2) NULL,
	TRANSACTION_CURRENCY_CODE VARCHAR(4) NULL,
	COMMODITY_CODE VARCHAR(20) NULL,
	STATISTICAL_CODE_DEPART VARCHAR(20) NULL,
	STATISTICAL_CODE_ARRIVAL VARCHAR(20) NULL,
	COMMODITY_CODE_SUPPLEMENTARY_UNIT VARCHAR(20) NULL,
	ITEM_QTY_SUPPLEMENTARY_UNIT SMALLINT NULL,
	TOTAL_ACTIVITY_SUPPLEMENTARY_UNIT VARCHAR(20) NULL,
	PRODUCT_TAX_CODE VARCHAR(20) NULL,
	DEPARTURE_CITY VARCHAR(100) NULL,
	DEPARTURE_COUNTRY VARCHAR(20) NULL,
	DEPARTURE_POST_CODE VARCHAR(20) NULL,
	ARRIVAL_CITY VARCHAR(100) NULL,
	ARRIVAL_COUNTRY VARCHAR(20) NULL,
	ARRIVAL_POST_CODE VARCHAR(20) NULL,
	SALE_DEPART_COUNTRY VARCHAR(20) NULL,
	SALE_ARRIVAL_COUNTRY VARCHAR(20) NULL,
	TRANSPORTATION_MODE VARCHAR(100) NULL,
	DELIVERY_CONDITIONS VARCHAR(20) NULL,
	SELLER_DEPART_VAT_NUMBER_COUNTRY VARCHAR(20) NULL,
	SELLER_DEPART_COUNTRY_VAT_NUMBER VARCHAR(20) NULL,
	SELLER_ARRIVAL_VAT_NUMBER_COUNTRY VARCHAR(20) NULL,
	SELLER_ARRIVAL_COUNTRY_VAT_NUMBER VARCHAR(20) NULL,
	TRANSACTION_SELLER_VAT_NUMBER_COUNTRY VARCHAR(20) NULL,
	TRANSACTION_SELLER_VAT_NUMBER VARCHAR(20) NULL,
	BUYER_VAT_NUMBER_COUNTRY VARCHAR(20) NULL,
	BUYER_VAT_NUMBER VARCHAR(20) NULL,
	VAT_CALCULATION_IMPUTATION_COUNTRY VARCHAR(20) NULL,
	TAXABLE_JURISDICTION VARCHAR(100) NULL,
	TAXABLE_JURISDICTION_LEVEL VARCHAR(20) NULL,
	VAT_INV_NUMBER VARCHAR(100) NULL,
	VAT_INV_CONVERTED_AMT DECIMAL (8,2) NULL,
	VAT_INV_CURRENCY_CODE VARCHAR(4) NULL,
	VAT_INV_EXCHANGE_RATE INTEGER NULL,
	VAT_INV_EXCHANGE_RATE_DATE DATE NULL,
	EXPORT_OUTSIDE_EU VARCHAR(20) NULL,
	INVOICE_URL TEXT NULL,
	BUYER_NAME TEXT NULL,
	ARRIVAL_ADRESS VARCHAR(120) NULL,
	SUPPLIER_NAME VARCHAR(120) NULL,
	SUPPLIER_VAT_NUMBER VARCHAR(20) NULL,
	TAX_REPORTING_SCHEME VARCHAR(20) NULL,
	TAX_COLLECTION_RESPONSIBILITY VARCHAR (20) Null,
)
